{
  "hosting": {
    "public": "website/dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "redirects": [
      {
        "source": "/docs",
        "destination": "/docs/yew_material",
        "type": 301
      }
    ]

  }
}
